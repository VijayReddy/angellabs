    <artifactId>angellabs-main</artifactId>
    <version>0.0.1-SNAPSHOT</version>
  </parent>
  <artifactId>angellabs</artifactId>
  <packaging>war</packaging>
  <name>angellabs</name>
  <description>angellabs</description>
  <dependencies>
  		<dependency>
			<groupId>co.angellabs</groupId>
			<artifactId>framework</artifactId>
			<version>framework</version>
		</dependency>
		<dependency>
			<groupId>co.angellabs</groupId>
			<artifactId>angellabs-entities</artifactId>
			<version>entities</version>
		</dependency>
		<dependency>
			<groupId>co.angellabs</groupId>
			<artifactId>profileservices</artifactId>
			<version>services</version>
		</dependency>
  
  </dependencies>
  
  <build>
		<finalName>Angellabs</finalName>
		<plugins>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-compiler-plugin</artifactId>
				<configuration>
					<source>1.5</source>
					<target>1.5</target>
				</configuration>
			</plugin>
		</plugins>
	</build>
  
  
</project>